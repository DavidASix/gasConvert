{
  "extends": "rallycoding",
  "rules": {
      "arrow-body-style": 0,
      "global-require": 0,
      "import/no-extraneous-dependencies": 0,
      "max-len": 0,
      "prefer-const": 0,
      "no-named-as-default-member": 0,
      "react/sort-comp": 0,
      "no-throw-literal": 0,
      "no-nested-ternary": 0
    }
}
